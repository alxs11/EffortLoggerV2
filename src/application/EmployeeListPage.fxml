<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ControllerEmployeeList">
   <children>
      <TableView fx:id="tableView" layoutY="39.0" prefHeight="400.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="firstNameColumn" prefWidth="168.79998779296875" text="First Name" />
          <TableColumn fx:id="lastNameColumn" prefWidth="133.60003662109375" text="Last Name" />
            <TableColumn fx:id="userNameColumn" prefWidth="154.39996337890625" text="Username" />
            <TableColumn fx:id="rankColumn" prefWidth="144.00006103515625" text="Rank" />
        </columns>
      </TableView>
      <ButtonBar prefHeight="38.0" prefWidth="600.0" style="-fx-background-color: #cccccc;">
         <buttons>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#goBack" text="Previous" />
            <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#signOut" text="Sign out" />
         </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
